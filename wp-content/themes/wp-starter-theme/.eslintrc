{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "class-methods-use-this": 0,
    "no-param-reassign": "warn",
    "no-undef": 1,
    "import/extensions": 0,
    "import/prefer-default-export": 0,
    "func-names": 0,
    "consistent-return": 0,
    "import/no-extraneous-dependencies": [
      "0",
      {
        "devDependencies": true
      }
    ],
    "import/no-unresolved": [
      0
    ],
    "no-console": "off"
  },
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  }
}
