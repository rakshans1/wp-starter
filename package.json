{
  "name": "wp-starter",
  "version": "0.0.1",
  "author": "Rakshan",
  "description": "wp-starter",
  "scripts": {
    "init": "./tools/install.sh",
    "install": "cd wp-content/themes/wp-starter-theme && npm install",
    "predev": "docker-compose -f docker-compose-dev.yml up -d",
    "dev": "cd wp-content/themes/wp-starter-theme && npm start",
    "start": "docker-compose up -d",
    "stop": "docker-compose -f docker-compose-dev.yml down",
    "build": "cd wp-content/themes/wp-starter-theme && gulp build"
  },
  "dependencies": {},
  "devDependencies": {}
}